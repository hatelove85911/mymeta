<dict>
    <key>KeepAlive</key>
    <false/>
    <key>Label</key>
    <string>syncmybin</string>
    <key>ProgramArguments</key>
    <array>
        <string>bash</string>
        <string>-c</string>
        <string>/usr/local/bin/unison -auto -batch -repeat watch ~/mymeta/home/bin ~/bin</string>
